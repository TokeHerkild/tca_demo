{
  "name": "Webto/{extKey}",
  "type": "typo3-cms-extension",
  "description": "{DESCRIPTION}",
  "homepage": "https://www.webto.dk",
  "license": ["GPL-2.0+"],
  "keywords": ["Webto"],
  "support": {
    "issues": "https://www.webto.dk"
  },
  "require": {
    "typo3/cms-core": "^8.7.0",
  },
  "autoload": {
    "psr-4": {
      "Webto\\{EXTNAME}\\": "Classes/"
    }
  },
  "replace": {
    "{extKey}": "self.version",
    "typo3-ter/{extKey}": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "{EXTKEY}"
    }
  }
}